    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.3",
    "ts-nameof": "^5.0.0",
    "ts-node": "^9.1.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.2"
  }
